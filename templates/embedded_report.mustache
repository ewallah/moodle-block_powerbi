{{!
    @template block_powerbi/embedded_report

    This template renders the embedded report.

    Example context (json):
    {
        "managereportsurl": "wwwroot/blocks/powerbi/report.php",
        "token": "randomtokenstring",
        "reportid": "reportidfrompowerbi",
        "groupid": "groupidfrompowerbi"
    }
}}
<div class="powerbi container-fluid">
  <div class="row">
      <div class="col-12">
          <div id="embedContainer" style="height: 50em;"></div>
          <div id="embedinfo" data-url="{{{embedurl}}}" data-report="{{reportid}}" data-token="{{{token}}}"></div>
      </div>
  </div>
  <div class="row">
    <div class="col-12">
      <a href="{managereportsurl}" class="btn btn-primary">{{#str}} back {{/str}}</a>
    </div>
  </div>
</div>
